  </PropertyGroup>

  <ItemGroup>
    <None Remove="CommandsValidator\" />
    <None Remove="CommandsValidator\Validators\" />
    <None Remove="Util\Hashes\" />
    <None Remove="CommandsExecutor\Controllers\Indexes\" />
    <None Remove="Journal\" />
    <None Remove="Journal\Models\" />
    <None Remove="Journal\Controllers\" />
    <None Remove="Journal\Controllers\Writers\" />
    <None Remove="Journal\Controllers\Reader\" />
